.message_box.mask {
    background-color: rgba(0, 0, 0, 0.7);
}
.message_box {
    height: 100%;
    left: 0;
    padding: 5 5px;
    position: fixed;
    top: 0;
    width: 100%;
    z-index: 1002;
}

.message_box > div {
    background-color: #ffffff;
    border-radius: 7px;
    font-size: 16px;
    max-width: 500px;
    position: relative;
    text-align: center;
    width: 100%;
}

.message_box .text {
    color: #000;
    line-height: 1.4;
    padding: 30px 15px;
}


.message_box .btns {
    line-height: 44px;
}
.b_top {
    border-width: 1px 0 0;
}

