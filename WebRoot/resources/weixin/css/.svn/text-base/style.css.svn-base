@charset "utf-8";
/* CSS Document */

/***************** 全局样式-start *****************/
html {
	width:100%;
	height:100%;
}
body {
	margin:0;
	padding:0;
	font-size:14px;
	width:100%;
	height:100%;
	font-family:"微软雅黑";
	color:#333;
	min-width:320px;
}
body, button, input, select, textarea {
	font: Tahoma, Helvetica, Arial, sans-serif;
	font-family:"微软雅黑";
}
div, form, img, ul, ol, li, dl, dt, dd, p {
	margin: 0;
	padding: 0;
	border: 0;
}
img {
	vertical-align:middle;
}
input {
	margin:0;
	padding:0;
}
li {
	list-style-type:none;
}
h1, h2, h3, h4, h5, h6 {
	margin:0;
	padding:0;
}
a {
	text-decoration:none;
	color:#333;
}
a:hover {
	color:#000;
}
.clear {
	clear:both;
	line-height:0px;
	margin:0px;
	padding:0px;
	height:0px;
	font-size:0px;
}
