@charset "utf-8";      
.pagination a {
    padding:6px 15px;
	border:1px solid #ebe1e2;
	display:inline-block;
	-moz-border-radius:2px;
	-webkit-border-radius:2px;
	border-radius:2px;
	
}
.pagination a:hover{
	background:#f0f0f0;
}
	

.pagination a, .pagination span {
    display: inline-block;
    padding: 6px 13px; 
    margin-right: 5px;
	margin-bottom: 5px;
}

.pagination .current {
    background: #423234;
    color: #fff;
	border: 1px solid #242a30;
	-moz-border-radius:2px;
	-webkit-border-radius:2px;
	border-radius:2px;
}

.pagination .current.prev, .pagination .current.next{
	color:#aaa;
	border-color:#ddd;
	background:#fff;
	-moz-border-radius:2px;
	-webkit-border-radius:2px;
	border-radius:2px;
}
.pagination,.pagination a{
	font-size:12px;
}
