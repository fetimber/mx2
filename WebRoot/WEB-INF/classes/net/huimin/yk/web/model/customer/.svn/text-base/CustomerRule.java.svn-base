package net.huimin.yk.web.model.customer;

import java.util.List;

public class CustomerRule {
    /**
     * 主键
     */
    private Integer id;

    /**
     * 规则名称
     */
    private String ruleName;

    private String ruleQuestion;
    
    private Integer ruleType;
    
	private List<CustomerChoice> choices;

	/**
     * @return 主键
     */
    public Integer getId() {
        return id;
    }

    /**
     * @param id 
	 *            主键
     */
    public void setId(Integer id) {
        this.id = id;
    }

    /**
     * @return 规则名称
     */
    public String getRuleName() {
        return ruleName;
    }

    /**
     * @param ruleName 
	 *            规则名称
     */
    public void setRuleName(String ruleName) {
        this.ruleName = ruleName;
    }

    public String getRuleQuestion() {
        return ruleQuestion;
    }

    public void setRuleQuestion(String ruleQuestion) {
        this.ruleQuestion = ruleQuestion;
    }
    
    public List<CustomerChoice> getChoices() {
		return choices;
	}

	public void setChoices(List<CustomerChoice> choices) {
		this.choices = choices;
	}
    public Integer getRuleType() {
		return ruleType;
	}

	public void setRuleType(Integer ruleType) {
		this.ruleType = ruleType;
	}
}