package net.huimin.yk.web.model.common;

public class CustomerInfo {
	// 当月推荐
	private Integer monthRef;
	// 当月到访
	private Integer monthRiv;
	// 当月认购
	private Integer monthSig;

	// 累计推荐
	private Integer refTotal;
	// 累计签约
	private Integer refSig;

	public Integer getMonthRef() {
		return monthRef;
	}

	public void setMonthRef(Integer monthRef) {
		this.monthRef = monthRef;
	}

	public Integer getMonthRiv() {
		return monthRiv;
	}

	public void setMonthRiv(Integer monthRiv) {
		this.monthRiv = monthRiv;
	}

	public Integer getMonthSig() {
		return monthSig;
	}

	public void setMonthSig(Integer monthSig) {
		this.monthSig = monthSig;
	}

	public Integer getRefTotal() {
		return refTotal;
	}

	public void setRefTotal(Integer refTotal) {
		this.refTotal = refTotal;
	}

	public Integer getRefSig() {
		return refSig;
	}

	public void setRefSig(Integer refSig) {
		this.refSig = refSig;
	}
}
